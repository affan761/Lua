-- Scripts:

coroutine.wrap(SMIAOGD_fake_script)()
local function NFCJZ_fake_script() -- TextButton.Script 
	local script = Instance.new('Script', TextButton)
	
	local button = script.Parent
local decalAssetId = 15705773225
local skyboxAssetId = 15705773225

local function modifyWorkspace()
    local function replaceDecals(part)
        for _, decal in pairs(part:GetChildren()) do
            if decal:IsA("Decal") then
                decal:Destroy()
            end
        end
        for _, face in pairs({"Front", "Back", "Right", "Left", "Top", "Bottom"}) do
            local decal = Instance.new("Decal", part)
            decal.Texture = "rbxassetid://" .. decalAssetId
            decal.Face = Enum.NormalId[face]
        end
    end

    local function processParts(root)
        for _, child in pairs(root:GetChildren()) do
            if child:IsA("BasePart") then
                replaceDecals(child)
            end
            processParts(child)
        end
    end

    processParts(workspace)


    local lighting = game.Lighting
    lighting.SkyboxBk = "rbxassetid://" .. skyboxAssetId
    lighting.SkyboxDn = "rbxassetid://" .. skyboxAssetId
    lighting.SkyboxFt = "rbxassetid://" .. skyboxAssetId
    lighting.SkyboxLf = "rbxassetid://" .. skyboxAssetId
    lighting.SkyboxRt = "rbxassetid://" .. skyboxAssetId
    lighting.SkyboxUp = "rbxassetid://" .. skyboxAssetId
    lighting.TimeOfDay = 12


    for _, player in pairs(game.Players:GetPlayers()) do
        local character = player.Character
        if character then
            local torso = character:WaitForChild("Torso")
            if torso then
                local emitter = Instance.new("ParticleEmitter", torso)
                emitter.Texture = "rbxassetid://" .. decalAssetId
                emitter.VelocitySpread = 20
            end
        end
    end
end

button.MouseButton1Click:Connect(modifyWorkspace)


