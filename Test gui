local Players = game:GetService("Players")
local player = Players.LocalPlayer

local decal = Instance.new("ScreenGui")
decal.Name = "decal"
decal.Parent = player:WaitForChild("PlayerGui")
decal.ResetOnSpawn = false
decal.ResetOnSpawn = true
decal.ZIndexBehavior = Enum.ZIndexBehavior.Global
decal.DisplayOrder = 0
decal.IgnoreGuiInset = false
decal.Enabled = true
decal.SelectionGroup = false

local TextButton = Instance.new("TextButton")
TextButton.Name = "TextButton"
TextButton.Parent = decal
TextButton.Size = UDim2.new(0, 200, 0, 50)
TextButton.Position = UDim2.new(0, 0, 0, 0)
TextButton.AnchorPoint = Vector2.new(0, 0)
TextButton.Visible = true
